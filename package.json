	},
	"homepage": "https://github.com/tynose/github-search-tool#readme",
	"dependencies": {
		"cors": "^2.8.5",
		"express": "^4.17.3",
		"express-graphql": "^0.7.1",
		"graphql": "^14.1.1",
		"node-fetch": "^2.3.0",
		"nodemon": "^1.18.10",
		"parse-link-header": "^1.0.1"
	},
	"devDependencies": {
		"concurrently": "^4.1.0"
	},
	"engines": {
		"node": "v11.4.0",
